{
  "name": "cdk-serverless-powertools-tracing-typescript-example",
  "description": "CDK Serverless Powertools Typescript Example to trace patient temperatures",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy": "cdk deploy -O ./cdk-exports.json",
    "cdk:hot": "cdk deploy --hotswap -O ./cdk-exports.json",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff ",
    "cdk:list": "cdk list",
    "cdk:synth": "cdk synth",
    "cdk:synth:output": "cdk synth --output=./templates",
    "cdk:synth:template": "cdk synth --no-staging > template.yaml",
    "sam:start": "sam local start-api"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.6.0",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.53.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.53.0-alpha.0",
    "@aws-lambda-powertools/commons": "^1.5.0",
    "@aws-lambda-powertools/logger": "^1.5.0",
    "@aws-lambda-powertools/metrics": "^1.5.0",
    "@aws-lambda-powertools/tracer": "^1.5.0",
    "@aws-sdk/client-dynamodb": "^3.218.0",
    "@aws-sdk/client-eventbridge": "^3.218.0",
    "@aws-sdk/lib-dynamodb": "^3.218.0",
    "@middy/core": "^4.0.0",
    "@middy/http-cors": "^4.0.0",
    "@middy/http-error-handler": "^4.0.0",
    "@middy/http-event-normalizer": "^4.0.0",
    "@middy/http-header-normalizer": "^4.0.0",
    "@middy/http-json-body-parser": "^4.0.0",
    "@middy/validator": "^4.0.0",
    "@types/aws-lambda": "^8.10.109",
    "aws-cdk-lib": "2.53.0",
    "aws-sdk": "^2.1264.0",
    "aws-xray-sdk-core": "^3.4.0",
    "constructs": "^10.1.174",
    "http-errors": "^2.0.0",
    "source-map-support": "^0.5.21",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.53.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/aws-lambda": "^8.10.109",
    "@types/eslint": "^8.4.10",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "aws-cdk": "^2.53.0",
    "esbuild": "^0.15.16",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript",
    "lambda",
    "tracing",
    "xray",
    "powertools"
  ],
  "tags": [
